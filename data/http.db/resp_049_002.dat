        function hideAllPopovers() {
            try {
                if (String($('isOnResultPopup').value).length < 1) {
                    try {
                        var previousRow = $('popOverCurrentResultId').value;
                        jQuery('#popOverCurrentResultId').val('');
                        if (previousRow != '') {
                            jQuery('#helpTextTipResults' + String(previousRow)).popover('hide');
                        }
                    }
                    catch (e) { }
                    try {
                        jQuery('#scmPopover').popover('hide');
                    }
                    catch (e) { }
                    jQuery('#isOnResultPopup').val('');
                }
                else {
                    jQuery('#isOnResultPopup').val('');
                }
            }
            catch (e) { }
            hideSubstratePopovers();
        }

        function hideSubstratePopovers() {
            if (String($('isOnSubstratePopup').value).length > 0) {
                if (String($('isOnSubstratePopup').value).substr(0, 1) == 'A') {
                    var rCount = parseInt(String($('isOnSubstratePopup').value).replace('A', ''));
                    if (rCount < 0) {
                        jQuery('#popOverTopSubstrate').popover('hide');
                        for (var i = 0; i < 10; i++) {
                            try {
                                jQuery('#conditionHelpTextTip' + String(i)).popover('hide');
                            }
                            catch (e) { }
                        }
                        for (var i = 0; i < 25; i++) {
                            try {
                                jQuery('#helpTextTip' + String(i)).popover('hide');
                            }
                            catch (e) { }
                        }

                    }
                    else {
                        try {
                            jQuery('#helpTextTip' + String(rCount)).popover('hide');
                        }
                        catch (e) { }
                        try {
                            jQuery('#conditionHelpTextTip' + String(rCount)).popover('hide');
                        }
                        catch (e) { }
                    }
                    jQuery('#isOnSubstratePopup').val('');
                }
                else {
                    jQuery('#isOnSubstratePopup').val('A' + $('isOnSubstratePopup').value);
                }
            }
        }

        // custom replace all function
        String.prototype.replaceAll = function (str1, str2, ignore) {
            return this.replace(new RegExp(str1.replace(/([\,\!\\\^\$\{\}\[\]\(\)\.\*\+\?\|\<\>\-\&])/g, function (c) { return "\\" + c; }), "g" + (ignore ? "i" : "")), str2);
        };

